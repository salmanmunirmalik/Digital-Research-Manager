{
  "name": "researchlabsync",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --exec tsx server/index.ts",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "build:backend": "tsc --project server/tsconfig.json",
    "db:setup": "tsx server/database/setup.ts",
    "build:all": "npm run build:backend && npm run build",
    "deploy:check": "npm run build:all && npm run start",
    "clean": "rm -rf dist node_modules package-lock.json",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "@google/genai": "^1.13.0",
    "@supabase/supabase-js": "^2.54.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.14.0",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "tsx": "^4.6.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
