        if (item.id === selectedInventoryItem.id) {
          let newQuantity = item.quantity;
          if (transactionForm.type === 'in') {
            newQuantity += transactionForm.quantity;
          } else if (transactionForm.type === 'out') {
            newQuantity -= transactionForm.quantity;
          } else if (transactionForm.type === 'adjustment') {
            newQuantity = transactionForm.quantity;
